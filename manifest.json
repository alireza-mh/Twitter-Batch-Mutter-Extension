{
  "name": "Batch Filter",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [{
    "js": ["contentscript.js"],
    "matches": ["<all_urls>"],
    "all_frames": true
  }],
  "permissions": [
    "activeTab",
    "storage"
  ],
  "web_accessible_resources": ["script.js" , "popup.js"]
}